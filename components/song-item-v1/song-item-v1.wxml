<!--components/song-item-v1/song-item-v1.wxml-->
<view class="song-item" bindtap="onSongItemTap">
  <image class="image" src="{{itemData.al.picUrl}}" />
  <view class="info">
    <view class="name">{{itemData.name}}</view>
    <view class="songer">{{itemData.ar[0].name}}</view>
  </view>
  <van-icon name="arrow" />
</view>